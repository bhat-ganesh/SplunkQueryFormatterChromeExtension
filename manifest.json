{
    "manifest_version": 2,
    "version": "0.0.1",
    "name": "Splunk Query Formatter",
    "author": "bhatganesh@gmail.com",
    "description": "One click to format Splunk query",
     "browser_action": {
        "default_icon": "images/icon.png"
    },
    "background":{
            "scripts":["/js/background.js"]
        },
    "permissions":[
        "https://*/splunk/*search*",
        "https://www.splunk.com/*search*"
        ]
}